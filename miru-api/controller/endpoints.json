{
    "user": {
        "path": "user",
        "urls": [
            {
                "url": "/user",
                "post": "create"
            },
            {
                "url": "/user/login",
                "post": "login"
            },
            {
                "url": "/user/confirmation/:token",
                "get": "confirmRegisteredUser"
            },
            {
                "url": "/user/confirmation/email",
                "post": "sendConfirmationEmail"
            }
        ]
    },
    "section": {
        "path": "section",
        "urls": [
            {
                "url": "/section",
                "get": "getAll"
            },
            {
                "url": "/section/:id",
                "get": "getSectionById"
            }
        ]
    }
}